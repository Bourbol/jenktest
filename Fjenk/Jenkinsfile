properties ([disableConcurrentBuilds()])

pipeline {
    agent any
    options {
        timestamps()
    }
    stages {
        stage("Just command") {
            steps {
                sh "mkdir /home/rinmamarun/grofalti"
            }
        }
        stage("Docker build") {
            steps {
                dir()
                sh "docker build ."
            }
        }
    }
}